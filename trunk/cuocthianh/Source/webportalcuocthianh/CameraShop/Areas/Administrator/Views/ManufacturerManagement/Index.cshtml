@{
    ViewBag.Title = "Nhà sản xuất";
}
<div class="head">
    <div class="isw-grid">
    </div>
    <h1>
        Danh sách nhà sản xuất</h1>
    <ul class="buttons">
        <li><a href="#" class="isw-settings"></a>
            <ul class="dd-list" style="display: none;">
             @if (CameraShop.BaseController.Permission.Contains("C"))
             {
                 
                <li><a href="@Url.Action("AddOrUpdate", "ManufacturerManagement")"><span class="isw-plus"></span>Thêm
                    nhà sản xuất</a></li>
                 
             }
            </ul>
        </li>
    </ul>
    <div class="clear">
    </div>
</div>
<div class="block-fluid table-sorting">
    <div class="row-fluid list">
     @if (CameraShop.BaseController.Permission.Contains("R"))
     {
        @Html.Action("_ShowAll", "ManufacturerManagement")
     }
     else
     {
         <span>Bạn không có quyền truy cập trang này</span> 
     }
    </div>
</div>


