@model List<CoreData.PictureExam>
@if (Model != null && Model.Count() > 0)
{<ul id="navlist">
    @foreach (var item in Model)
    {
        <li><a class="tooltip picture-small" href="/Media/PictureExam/@item.Image" title="@item.Title">
            <img src="/Media/PictureExam/@item.Image" width="68" height="68" alt="">
        </a></li>
    }
    </ul>
}